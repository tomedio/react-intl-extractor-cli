  "description": "CLI tool to extract messages JSON for react-intl library with smart merging",
  "keywords": [
    "cli",
    "globalization",
    "intl",
    "internationalization",
    "i18n",
    "locale",
    "localization",
    "react",
    "react-intl",
    "reactjs",
    "translate",
    "translation"
  ],
  "author": "Tomasz Bajorek <tomasz.bajorek@tomedio.com",
  "homepage": "https://github.com/tomedio/react-intl-extractor-cli",
  "license": "MIT",
  "bin": {
    "intl_extractor": "bin/intl_extractor"
  },
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/tomedio/react-intl-extractor-cli.git"
  },
  "bugs": {
    "url": "https://github.com/tomedio/react-intl-extractor-cli/issues"
