    "@ungap/structured-clone": "^1.0.1",
    "@vue/compiler-sfc": "^3.2.45",
    "chalk": "^5.2.0",
    "commander": "^9.5.0",
    "fast-glob": "^3.2.12",
    "fs-extra": "^11.1.0",
    "json-stable-stringify": "^1.0.2",
    "loud-rejection": "^2.2.0",
    "path": "^0.12.7",
    "typescript": "^4.9.4",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "scripts": {
    "build:cli": "webpack --progress",
